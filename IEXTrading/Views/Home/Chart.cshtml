@model IEXTrading.Models.ViewModel.CompaniesEquities


<head>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/1.0.2/Chart.min.js"></script>
    <script language="JavaScript">
        function displayLineChart() {
            var data = {
                labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                datasets: [
                    {
                        fillColor: "rgba(0,200,255,0.5)",
                        strokeColor: "rgba(0,200,25s5,1)",
                        pointColor: "rgba(220,220,220,1)",
                        pointStrokeColor: "#fff",
                        pointHighlightFill: "#fff",
                        pointHighlightStroke: "rgba(220,220,220,1)",
                        data: [1424, 4522, 3977, 4991, 7238, 9443, 8379, 7134, 3245, 2599, 1310, 1168]
                    }
                ]
            };
            var ctx = document.getElementById("lineChart").getContext("2d");
            var options = {};
            var lineChart = new Chart(ctx).Bar(data, options);
        }
    </script>
</head>
<body onload="displayLineChart();">
    <!-- Header of the page -->
    <h1 class="head-one">Storm Events Location - 2019</h1>
    <div class="logo">
        <img src="img/usf-logo-png-2.png" alt="img/placeholder.png" height="150">
    </div>

    <div class="vis-info">
        <p class="vis-para">Following bar graph shows the trend of storms throughout the year. It shows the count of storms for each month of 2019.</p>
    </div>
    <!-- visualization one begins -->
    <div class="box">
        <h2 class="head-two">Monthly count of storm events - 2019</h2>
        <canvas id="lineChart" height="540" width="960"></canvas>
    </div>

    <!-- visualization one ends -->
</body>
</html>